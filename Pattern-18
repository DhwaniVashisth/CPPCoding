#include <iostream>
using namespace std;
int main()
{
    int i=1,n;
    cin>>n;
    
    while(i<=n){
        int space=i-1;
        int count=n-i+1;
        while(space){
            cout<<" ";
            space--;
        }
        while(count){
            cout<<i;
            count--;
        }
        cout<<endl;
        i=i+1;
    }
    return 0;
}
