#include <iostream>
using namespace std;
int main()
{
    int i=1,n;
    cin>>n;
    while(i<=n){
        int j=1;
        char count;
        while(j<=n){
            count= 'A'+i+j-2;
            cout<<count<<" ";
            count=count+1;
            j=j+1;
        }
        cout<<endl;
        i=i+1;
    }
    return 0;
}
