#include <iostream>
using namespace std;
int main()
{
    int i=1,n;
    cin>>n;
    
    while(i<=n){
        int j=1;
        int space=n-i;
        while(space){
            cout<<" ";
            space--;
        }
        while(j<=i){
            cout<<i;
            j++;
        }
        cout<<endl;
        i=i+1;
    }
    return 0;
}
